pipeline {

    agent any

    stages {
        stage('build deploy'){
            steps {
                echo 'Hello World!'
            }
        }
        stage('Unit Test'){
                    steps {
                        echo 'Hello Unit!'
                    }
                }
        stage('Kubernets'){
                    steps {
                        echo 'Hello Kubernets!'
                    }
                }
        stage('Automation'){
                    steps {
                        echo 'Hello Automation Script!'
                    }
             }
        stage('Done'){
                            steps {
                                echo 'Hello Automation Script!'
                            }
                     }
    }

}